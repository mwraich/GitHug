<div id= "imageslideshow">
  <%= link_to image_tag('left_arrow.png'), '#', class:'left_arrow'%>
  <ul>
    <% range = (0..(@profile.images.length)) %>
    <% range.select do |r| %>
    <%= image_tag(@profile.images[r-1].image_url) %>
    <% end %>
</ul>
<%= link_to image_tag('right_arrow.png'), '#', class:'right_arrow'%>
</div>
