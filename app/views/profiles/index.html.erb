<h1> User Search </h1>


<label>What are you looking for?</label><br>
<input type="radio" name="choice" value="paired"> Paired <br>
<input type="radio" name="choice" value="date"> Date <br>


  <%= render 'pair_search' %>
  <%= render 'date_search' %>

<% @profiles.each do |profile| %>
<%= image_tag profile.image, class: 'profile-image' %>
<div id="index-view-profile-info">
  <p><%= profile.first_name%>, <%=profile.age %></p>
</div>
<!--Once languages added will update this line-->
<p>Ruby, JavaScript, CSS</p>

<hr class="line-break">
<% end %>
